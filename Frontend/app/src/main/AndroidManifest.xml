<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.With_Runn"
        tools:targetApi="31" >

        <meta-data
            android:name="com.naver.maps.map.NCP_KEY_ID"
            android:value="${naverMapClientId}"/>

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ui.friend.FriendDetailActivity"
            android:exported="false" />

        <activity
            android:name=".ui.friend.AllFriendsActivity"
            android:exported="false" />

        <activity
            android:name=".ui.chat.ChatActivity"
            android:exported="false" />

        <activity
            android:name=".ui.chat.ChatRoomActivity"
            android:exported="false" />

        <activity android:name=".ui.onboarding.OnboardingActivity" />
        <activity android:name=".ui.mypage.MypageFollowersActivity" />
    </application>

</manifest>